{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"compile:watch\"",
    "compile": "babel ./src --out-dir dist --extensions .ts,.tsx",
    "compile:watch": "babel -w ./src --out-dir dist --extensions .ts,.tsx",
    "build": "next build",
    "start": "next start",
    "m:create": "typeorm migration:create",
    "m:run": "typeorm migration:run",
    "m:revert": "typeorm migration:revert",
    "e:create": "typeorm entity:create"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "github-markdown-css": "^4.0.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "marked": "^2.1.3",
    "md5": "^2.3.0",
    "next": "^11.0.1",
    "next-iron-session": "^4.2.0",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.34",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@styled-jsx/plugin-sass": "^3.0.0",
    "@types/lodash": "^4.14.171",
    "@types/marked": "^2.0.4",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/ua-parser-js": "^0.7.36",
    "concurrently": "^6.2.0",
    "file-loader": "^6.2.0",
    "next-images": "^1.8.1",
    "sass": "^1.35.2",
    "typescript": "^4.3.5"
  }
}
